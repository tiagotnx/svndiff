{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch svndiff",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": ["--help"],
      "env": {},
      "showLog": true,
      "preLaunchTask": "Build"
    },
    {
      "name": "Launch with config file",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": ["--config", "config.yaml"],
      "env": {},
      "showLog": true,
      "preLaunchTask": "Build"
    },
    {
      "name": "Launch with flags",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "--urlA", "https://svn.example.com/branchA",
        "--revsA", "123",
        "--urlB", "https://svn.example.com/branchB",
        "--revsB", "124",
        "--output", "json"
      ],
      "env": {},
      "showLog": true,
      "preLaunchTask": "Build"
    },
    {
      "name": "Debug Tests",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceFolder}",
      "args": ["-test.v"],
      "showLog": true
    },
    {
      "name": "Debug Package Tests",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${fileDirname}",
      "args": ["-test.v"],
      "showLog": true
    }
  ]
}
