# Arquivo de configuração de exemplo para svndiff
# Este arquivo demonstra todas as opções disponíveis

# Configuração da Branch A
branchA:
  url: "https://svn.example.com/project/branches/feature-A"
  revisions:
    - "12345"
    - "12348"
    - "12350"

# Configuração da Branch B  
branchB:
  url: "https://svn.example.com/project/branches/feature-B"
  revisions:
    - "12351"
    - "12355"

# Formato de saída: list, diff ou json
output: "list"

# Mostrar apenas resumo das diferenças (true) ou diff completo (false)
summarize: true

# Credenciais de autenticação (opcional)
auth:
  user: "myuser"
  password: "mypassword"
